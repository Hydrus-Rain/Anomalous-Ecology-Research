<?xml version='1.0' encoding='utf-8'?>
<Defs>
  <ThingDef ParentName="PlantBase">
    <defName>AER_Genobulb_Bush</defName>
    <label>Genobulb bush</label>
    <description>A short bush known for yielding fist-sized berries called Genobulbs, extremely hardy and capable of growing even in rocky soil.</description>
    <thingClass>Plant</thingClass>
    <altitudeLayer>LowPlant</altitudeLayer>
    <selectable>true</selectable>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Plants/Primal/Genobulb_Bush</texPath>
      <shaderType>CutoutPlant</shaderType>
      <drawSize>(2.0,2.0)</drawSize>
    </graphicData>
    <statBases>
      <Beauty>1</Beauty>
      <Flammability>1</Flammability>
      <MaxHitPoints>40</MaxHitPoints>
    </statBases>
    <plant>
      <fertilityMin>0.7</fertilityMin>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <growDays>4.6</growDays>
      <harvestedThingDef>AER_Genobulbs</harvestedThingDef>
      <harvestYield>6</harvestYield>
      <harvestWork>200</harvestWork>
      <sowMinSkill>5</sowMinSkill>
      <sowTags>
        <li>Ground</li>
      </sowTags>
      <visualSizeRange>0.3~2.0</visualSizeRange>
      <wildBiomes>
        <AER_PrimalJungle>0.5</AER_PrimalJungle>
      </wildBiomes>
      <wildClusterRadius>4.0</wildClusterRadius>
      <wildClusterWeight>85</wildClusterWeight>
      <wildOrder>2</wildOrder>
    </plant>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
      <foodType>Plant</foodType>
    </ingestible>
    <thingCategories>
      <li>Plants</li>
    </thingCategories>
    <lifeStages>
      <li>
        <minGrowth>0.0</minGrowth>
        <label>seedling</label>
        <statBases>
          <Beauty>-1</Beauty>
        </statBases>
        <graphicData>
          <graphicClass>Graphic_Multi</graphicClass>
          <texPath>Things/Plants/Primal/Genobulb_Bush_Immature</texPath>
          <shaderType>CutoutPlant</shaderType>
          <drawSize>(1.0,1.0)</drawSize>
        </graphicData>
      </li>
      <li>
        <minGrowth>0.5</minGrowth>
        <label>growing</label>
        <statBases>
          <Beauty>0</Beauty>
        </statBases>
        <graphicData>
          <graphicClass>Graphic_Multi</graphicClass>
          <texPath>Things/Plants/Primal/Genobulb_Bush</texPath>
          <shaderType>CutoutPlant</shaderType>
          <drawSize>(1.5,1.5)</drawSize>
        </graphicData>
      </li>
      <li>
        <minGrowth>0.8</minGrowth>
        <label>harvestable</label>
        <statBases>
          <Beauty>1</Beauty>
        </statBases>
        <graphicData>
          <graphicClass>Graphic_Multi</graphicClass>
          <texPath>Things/Plants/Primal/Genobulb_Bush_Harvest</texPath>
          <shaderType>CutoutPlant</shaderType>
          <drawSize>(1.8,1.8)</drawSize>
        </graphicData>
      </li>
      <li>
        <minGrowth>1.0</minGrowth>
        <label>mature</label>
        <statBases>
          <Beauty>2</Beauty>
        </statBases>
        <graphicData>
          <graphicClass>Graphic_Multi</graphicClass>
          <texPath>Things/Plants/Primal/Genobulb_Bush_Mature</texPath>
          <shaderType>CutoutPlant</shaderType>
          <drawSize>(2.0,2.0)</drawSize>
        </graphicData>
      </li>
    </lifeStages>
  </ThingDef>
</Defs>