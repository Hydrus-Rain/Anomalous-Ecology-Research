<?xml version='1.0' encoding='utf-8'?>
<Defs>
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>AER_Genobulbs</defName>
    <label>Genobulbs</label>
    <description>Strange fist-sized berries that yield a compound known to stabilize genes. Taste is comparable to a mix between a peach and an orange.</description>
    <stackLimit>10</stackLimit>
    <graphicData>
      <graphicClass>Graphic_StackCount</graphicClass>
      <texPath>Things/Items/Primal/Genobulb</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.2</MarketValue>
      <Mass>0.027</Mass>
      <Nutrition>0.35</Nutrition>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>14</daysToRotStart>
        <rotDestroys>false</rotDestroys>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
  </ThingDef>
</Defs>