<?xml version='1.0' encoding='utf-8'?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>AER_FernstalkerEggFertilized</defName>
    <label>Fernstalker Egg (fert.)</label>
    <description>A fertilized Fernstalker egg. If all goes well, it should hatch into a baby fernstalker. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <texPath>Things/Items/Primal/Eggs/Fernstalker_Egg</texPath>
      <drawSize>0.9</drawSize>
	  <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <Nutrition>0.35</Nutrition>
      <MarketValue>21.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>6</hatcherDaystoHatch>
        <hatcherPawn>AER_Fernstalker</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
  <ThingDef ParentName="EggUnfertBase">
    <defName>AER_FernstalkerEggUnfertilized</defName>
    <label>Fernstalker Egg (unfert.)</label>
    <description>An unfertilized Fernstalker egg. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <texPath>Things/Items/Primal/Eggs/Fernstalker_Egg</texPath>
      <drawSize>0.9</drawSize>
	  <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>11</MarketValue>
    </statBases>
  </ThingDef>
</Defs>