<?xml version='1.0' encoding='utf-8'?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>AER_ViridianRaptorEggFertilized</defName>
    <label>Viridian Raptor Egg (fert.)</label>
    <description>A fertilized Viridian Raptor egg. If all goes well, it should hatch into a baby velociraptor. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <texPath>Things/Items/Primal/Eggs/ViriRaptor_Egg</texPath>
      <drawSize>0.9</drawSize>
	  <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <Nutrition>0.35</Nutrition>
      <MarketValue>21.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>6</hatcherDaystoHatch>
        <hatcherPawn>AER_ViridianRaptor</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
    <ThingDef ParentName="EggUnfertBase">
    <defName>AER_ViridianRaptorEggUnfertilized</defName>
    <label>Viridian Raptor Egg (unfert.)</label>
    <description>An unfertilized Viridian Raptor egg. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <texPath>Things/Items/Primal/Eggs/ViriRaptor_Egg</texPath>
      <drawSize>0.9</drawSize>
	  <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>11</MarketValue>
    </statBases>
  </ThingDef>
</Defs>